      alert(erros);\n            });\n    }\n\n    if (result) {\n        controlComponent = result.map(book => (\n            <ContainerImageOne >\n                <Teste>\n                    <ImageBook src={book.volumeInfo.imageLinks !== undefined ? book.volumeInfo.imageLinks.thumbnail : ''} alt={book.title} />\n                </Teste>\n                <hr />\n                <ContainerDescription>\n                    <TitleName>\n                        Livro:\n                        <TitleBook>{book.volumeInfo.title}</TitleBook>\n                    </TitleName>\n\n                    <TitleAuthorName>\n                        Autor:\n                        <TitleAuthor>\n                            {book.volumeInfo.authors && book.volumeInfo.authors}\n                            {!book.volumeInfo.authors && \"Não Informado\"}\n                        </TitleAuthor>\n                    </TitleAuthorName>\n                </ContainerDescription>\n\n                <hr />\n\n                {book.saleInfo.buyLink &&\n\n                    <LinkCompra href={book.saleInfo.buyLink} target=\"_blank\">\n                        <Button style={{ alignItems: 'center', textAlign: 'center', marginLeft: '30px' }} variant=\"success\">Comprar <FontAwesomeIcon icon={faShoppingCart} /></Button>\n                    </LinkCompra>\n                }\n                {!book.saleInfo.buyLink &&\n                    <InfoNotBuy>Indisponível</InfoNotBuy>\n                }\n            </ContainerImageOne>\n        ))\n    } else {\n        controlComponent = <BookNotFound />\n    }\n\n    return (\n        <Container>\n            <ContainerJumbotron>\n                <Jumbotron style={{ paddingTop: \"20px\", paddingBottom: '20px' }}>\n                    <Form onSubmit={handleSearch}>\n                        <Form.Group>\n                            <Form.Label style={{ fontWeight: 'bold' }}>Pesquise seu livro:</Form.Label>\n                            <Form.Control type=\"text\" onChange={handleChange} value={nameBook} />\n                        </Form.Group>\n                        <Form.Group>\n                            <Button onClick={handleClickLoading} variant=\"success\" type=\"submit\" data-testid=\"btn-pesquisar\">\n                                {isLoading ? 'Carregando…' : 'Pesquisar'}\n                            </Button>\n                        </Form.Group>\n                    </Form>\n                </Jumbotron>\n            </ContainerJumbotron>\n            <ContainerImage>\n                {controlComponent}\n            </ContainerImage>\n        </Container>\n    );\n}\n",["42","43"],"/var/www/thiagoScannerLivros/frontend/src/reportWebVitals.js",[],"/var/www/thiagoScannerLivros/frontend/src/components/App/index.js",["44"],"import React from 'react';\nimport Search from '../Search';\n\nimport {\n    Container\n} from './styled';\n\nexport default () =>{\n    return (\n        <Container>\n            <Search />            \n        </Container>\n    );\n}",["45","46"],"/var/www/thiagoScannerLivros/frontend/src/index.js",[],["47","48"],"/var/www/thiagoScannerLivros/frontend/src/components/BookNotFound/index.js",["49","50"],"import React from 'react';\n\nimport {\n    Container,\n    Title\n} from './styled';\n\n\nexport default () =>{\n    return(\n        <Container>\n            Livro Não Encontrado\n        </Container>\n    );\n\n}","/var/www/thiagoScannerLivros/frontend/src/components/BookNotFound/styled.js",[],{"ruleId":"51","severity":1,"message":"52","line":24,"column":1,"nodeType":"53","endLine":116,"endColumn":2},{"ruleId":"54","severity":1,"message":"55","line":28,"column":20,"nodeType":"56","messageId":"57","endLine":28,"endColumn":29},{"ruleId":"58","replacedBy":"59"},{"ruleId":"60","replacedBy":"61"},{"ruleId":"51","severity":1,"message":"52","line":8,"column":1,"nodeType":"53","endLine":14,"endColumn":2},{"ruleId":"58","replacedBy":"62"},{"ruleId":"60","replacedBy":"63"},{"ruleId":"58","replace